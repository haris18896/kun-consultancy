{
    "name": "rustic",
    "description": "Rustic - React Business Template",
    "author": "HasTech",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "node server.js",
        "build": "next build",
        "start": "NODE_ENV=production node server.js",
        "export": " next export -o build",
        "lint": "next lint",
        "move": "mv ./build/_next ./build/next",
        "change": "cd build",
        "replace": "grep -rl \"_next\" ./build | xargs sed -i \"s/_next/next/g\"",
        "back": "cd ../",
        "remove": "rm -rf .next/ build/ out/ extension.zip",
        "git": "git add * && git commit -m",
        "shoot": "npm run build && npm run export && npm run move && npm run change && npm run replace && npm run back ",
        "postgit": "git push --all"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.8.2",
        "@sendgrid/mail": "^7.7.0",
        "axios": "^0.27.2",
        "classnames": "^2.3.1",
        "cloudinary-build-url": "^0.2.4",
        "clsx": "^1.1.1",
        "disqus-react": "^1.1.2",
        "dotenv": "^16.0.1",
        "emailjs": "^4.0.0",
        "eslint": "^8.5.0",
        "eslint-config-next": "^12.0.7",
        "formik": "^2.2.9",
        "gray-matter": "^4.0.3",
        "next": "12.0.10",
        "next-redux-wrapper": "^7.0.5",
        "node-sass": "^6.0.1",
        "nodemailer": "^6.7.7",
        "nprogress": "^0.2.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-icons": "^4.3.1",
        "react-markdown": "^7.1.1",
        "react-redux": "^8.0.2",
        "redux": "^4.2.0",
        "redux-devtools-extension": "^2.13.9",
        "redux-thunk": "^2.4.1",
        "swiper": "^6.8.4",
        "webpack": "^5.53.0",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.0",
        "postcss": "^8.4.5",
        "tailwindcss": "^3.0.7"
    },
    "browser": {
        "fs": false,
        "path": false,
        "os": false
    }
}
